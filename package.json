{
  "name": "noexcel",
  "version": "1.0.5",
  "description": "Node module to allow for easy Excel file creation without consideration of render detail",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "build": "tsc",
    "test": "./node_modules/.bin/mocha -r ./node_modules/ts-node/register test/**.test.ts",
    "publish" :"npm run build && npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenisad5566/noexcel.git"
  },
  "keywords": [
    "node",
    "excel",
    "xlsx",
    "xls"
  ],
  "author": "kenisad5566",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenisad5566/noexcel/issues"
  },
  "homepage": "https://github.com/kenisad5566/noexcel#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/lodash": "^4.14.169",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.3.0",
    "@types/node-xlsx": "^0.15.1",
    "husky": "^6.0.0",
    "mocha": "^8.4.0",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "excel4node": "^1.7.2",
    "node-xlsx": "^0.16.1"
  }
}
